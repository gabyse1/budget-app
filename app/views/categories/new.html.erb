<%= render partial: 'layouts/header', locals: { h_title: 'CATEGORY', h_submit: true } %>
<div class="form__container">
<%= form_with model: Category.new do |f| %>
  <%= f.text_field :name, placeholder: 'Name' %>
  <div class="field">
  <%= f.label :icon %>
  <%= f.file_field :icon %>
  </div>
  <%= f.submit 'Save', class: 'btn-header' %>
<% end %>
</div>